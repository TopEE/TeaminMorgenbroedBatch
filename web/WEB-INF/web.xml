<?xml version="1.0" encoding="UTF-8"?>
<web-app xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
    <servlet>
        <servlet-name>JobSubmitterServlet</servlet-name>
        <servlet-class>com.teamin.batch.servlet.JobSubmitterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JobSubmitterServlet</servlet-name>
        <url-pattern>/JobSubmitterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UtilServlet</servlet-name>
        <servlet-class>com.teamin.batch.servlet.UtilServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UtilServlet</servlet-name>
        <url-pattern>/UtilServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
